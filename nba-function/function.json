{
  "scriptFile": "main.py",
  "bindings": [
    {
      "route": "{*route}",
      "authLevel": "Anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "methods": [
        "GET",
        "POST",
        "DELETE",
        "PUT"
      ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "$return"
    }
  ]
}